<%= render 'layouts/adminheader' %>
<h1>Admin Users</h1>
<table class="table table-striped table-hover">
  <thead>
  <td>Id</td>
  <td>Username</td>
  <td>E-mail</td>
  <td>Role</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.username%></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
    </tr>

  <% end %>
</table>